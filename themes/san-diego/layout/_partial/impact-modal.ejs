<!-- Impact Modal -->
<div id="impact-modal" class="impact-modal" style="display: none;">
    <div class="impact-modal-backdrop" onclick="closeImpactModal()"></div>
    <div class="impact-modal-sheet">
        <div class="impact-modal-header">
            <button class="impact-modal-close minimal-close" onclick="closeImpactModal()" aria-label="Close impact report">
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20" fill="none">
                    <path d="M15 5L5 15M5 5l10 10" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
            </button>
            <div class="impact-modal-title">Design Impact Report 2025</div>
            <button class="impact-modal-party-mode" onclick="togglePartyMode()" aria-label="Toggle party mode" title="Activate party mode">
                <svg class="party-icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M5 3v4M3 5h4M6 17v4m-2-2h4m5-16l2.286 6.857L21 12l-5.714 2.143L13 21l-2.286-6.857L5 12l5.714-2.143L13 3z"/>
                </svg>
                <span class="party-text">Party</span>
                <audio id="impact-audio" loop>
                    <source src="<%- url_for('/media/deepHouse.mp3') %>" type="audio/mpeg">
                </audio>
            </button>
        </div>
		<div class="impact-modal-subtitle">Strategic design leadership through collaboration, innovation, and measurable business impact.</div>
        <div class="impact-grid">
            <!-- FINANCIAL IMPACT Section -->
            <div class="impact-section" data-section="financial">
                <div class="section-label">FINANCIAL IMPACT</div>
                
                <!-- Total Impact - Large tile spanning 3x2 -->
                <div class="impact-tile tile-large tile-revenue" data-animate="counter" style="grid-column: span 3; grid-row: span 2;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">
									<span class="unit">$</span>
                                    <span class="counter" data-target="30">0</span>
                                    <span class="unit">M+</span>
                                </div>
                                <div class="tile-label">Revenue Created</div>
                            </div>
                        </div>
                        <div class="tile-detail">Saved at-risk revenue + new recurring revenue</div>
                    </div>
                </div>

                <!-- Annual Savings - Medium tile -->
                <div class="impact-tile tile-medium tile-savings" data-animate="counter" style="grid-column: span 3;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">
                                    <span class="unit">$</span>
                                    <span class="counter" data-target="8">0</span>
                                    <span class="unit">M/yr</span>
                                </div>
                                <div class="tile-label">Yearly Savings Delivered</div>
                            </div>
                        </div>
                        <div class="tile-detail">Engineering & operational efficiency</div>
                    </div>
                </div>

                <!-- Engineering Hours Saved - Small tile -->
                <div class="impact-tile tile-medium tile-hours" data-animate="counter" style="grid-column: span 3;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">
                                    <span class="unit">$</span>
                                    <span class="counter" data-target="1.2">0</span>
                                    <span class="unit">M/yr</span>
                                </div>
                                <div class="tile-label">Engineering Productivity Impact</div>
                            </div>
                        </div>
                        <div class="tile-detail">Design system & tooling efficiency</div>
                    </div>
                </div>
            </div>

            <!-- EFFICIENCY GAINS Section -->
            <div class="impact-section" data-section="efficiency">
                <div class="section-label">EFFICIENCY GAINS</div>
                
                <!-- Process Acceleration - Medium tile -->
                <div class="impact-tile tile-medium tile-efficiency" data-animate="flip" style="grid-column: span 2;">
                    <div class="tile-inner">
                        <div class="tile-front">
                            <div class="tile-mobile-wrapper">
                                <div class="tile-mobile-content">
                                    <div class="tile-value">250×</div>
                                    <div class="tile-label">Speed Improvement</div>
                                </div>
                            </div>
                        </div>
                        <div class="tile-back">
                            <div class="tile-detail">Content publishing: weeks to hours</div>
                        </div>
                    </div>
                </div>

                <!-- Support Reduction - Large tile -->
                <div class="impact-tile tile-large tile-support" data-animate="wave" style="grid-column: span 2; grid-row: span 2;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">85%</div>
                                <div class="tile-label">Support Tickets Reduced</div>
                            </div>
                        </div>
                        <div class="tile-detail">From 1,200 to 180 per month</div>
                    </div>
                </div>

                <!-- UI Ticket Reduction - Small tile -->
                <div class="impact-tile tile-small tile-tickets" data-animate="grow">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">90%</div>
                                <div class="tile-label">Design Issues Prevented</div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Deployment Time - Small tile -->
                <div class="impact-tile tile-small tile-deployment" data-animate="race">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">90%</div>
                                <div class="tile-label">Deployment Speed</div>
                            </div>
                        </div>
                        <div class="tile-detail">From 8 hours to 45 minutes</div>
                    </div>
                </div>
                <!-- Infrastructure Reduction - Medium tile -->
                <div class="impact-tile tile-medium tile-infra" data-animate="scale" style="grid-column: span 2;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">95%</div>
                                <div class="tile-label">System Issues Eliminated</div>
                            </div>
                        </div>
                        <div class="tile-detail">Automated away most problems</div>
                    </div>
                </div>
                <!-- Dev Velocity - Small tile -->
                <div class="impact-tile tile-small tile-velocity" data-animate="grow">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">5×</div>
                                <div class="tile-label">Developer Speed</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- PLATFORM SCALE Section -->
            <div class="impact-section" data-section="scale">
                <div class="section-label">PLATFORM SCALE</div>
                <!-- Products Unified - Large tile -->
                <div class="impact-tile tile-large tile-systems" data-animate="stack" style="grid-column: span 6;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">100+</div>
                                <div class="tile-label">Products Modernized</div>
                            </div>
                        </div>
                        <div class="tile-detail">Unified under one system</div>
                    </div>
                </div>

                <!-- Storage Eliminated - Medium tile -->
                <div class="impact-tile tile-medium tile-storage" data-animate="shine" style="grid-column: span 3;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">12TB</div>
                                <div class="tile-label">Customer Storage Saved</div>
                            </div>
                        </div>
                        <div class="tile-detail">Reduced redundancy</div>
                    </div>
                </div>
				<!-- Platform Adoption - Medium tile -->
			<div class="impact-tile tile-medium tile-adoption" data-animate="counter" style="grid-column: span 3;">
				<div class="tile-inner">
					<div class="tile-mobile-wrapper">
						<div class="tile-mobile-content">
							<div class="tile-value">
								<span class="counter" data-target="45">0</span>
									<span class="unit">K</span>
							</div>
							<div class="tile-label">Monthly Active Operations</div>
						</div>
					</div>
					<div class="tile-detail">15K+ tasks automated daily</div>
				</div>
			</div>
            </div>
            <!-- BEHAVIORAL IMPACT Section -->
            <div class="impact-section" data-section="behavioral">
                <div class="section-label">BEHAVIORAL IMPACT</div>
                
                <!-- Conversion Rate - Large tile -->
                <div class="impact-tile tile-large tile-conversion" data-animate="wave" style="grid-column: span 2; grid-row: span 2;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">22%</div>
                                <div class="tile-label">Conversion to Savings</div>
                            </div>
                        </div>
                        <div class="tile-detail">Nearly 2x industry average</div>
                    </div>
                </div>

                <!-- Borrower Savings - Large tile -->
                <div class="impact-tile tile-large tile-borrower" data-animate="shine" style="grid-column: span 2; grid-row: span 2;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">$1.3M</div>
                                <div class="tile-label">Total saved for families</div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- CTR Increase - Medium tile -->
                <div class="impact-tile tile-medium tile-ctr" data-animate="spark" style="grid-column: span 2;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">75%</div>
                                <div class="tile-label">Avg Click Rate Improvement</div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Retention - Medium tile -->
                <div class="impact-tile tile-medium tile-retention" data-animate="pulse" style="grid-column: span 2;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">85%</div>
                                <div class="tile-label">User Retention Rate</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            
            <!-- CROSS-FUNCTIONAL COLLABORATION Section -->
            <div class="impact-section" data-section="collaboration">
                <div class="section-label">CROSS-FUNCTIONAL COLLABORATION</div>
                
                <!-- Engineering Partnership - Large tile -->
                <div class="impact-tile tile-large tile-engineering" data-animate="stack" style="grid-column: span 3; grid-row: span 2;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">Deep Partnership</div>
                                <div class="tile-label">Engineering Collaboration</div>
                            </div>
                        </div>
                        <div class="tile-detail">Embedded in technical decisions, not request fulfillment</div>
                    </div>
                </div>
                
                <!-- Multi-Team Collaboration - Medium tile -->
                <div class="impact-tile tile-medium tile-teams" data-animate="pulse" style="grid-column: span 3;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">Cross-Team</div>
                                <div class="tile-label">Design Collaboration</div>
                            </div>
                        </div>
                        <div class="tile-detail">Design feedback across Autodesk teams</div>
                    </div>
                </div>
                
                <!-- Workshop Facilitation - Medium tile -->
                <div class="impact-tile tile-medium tile-workshops" data-animate="spark" style="grid-column: span 2;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">Facilitator</div>
                                <div class="tile-label">Workshop Design</div>
                            </div>
                        </div>
                        <div class="tile-detail">Internal teams + customers</div>
                    </div>
                </div>
                
                <!-- Code Contribution - Small tile -->
                <div class="impact-tile tile-small tile-code" data-animate="flip">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">3</div>
                                <div class="tile-label">Languages</div>
                            </div>
                        </div>
                        <div class="tile-detail">React, JS, Python</div>
                    </div>
                </div>
            </div>
            
            <!-- STRATEGIC INNOVATION Section -->
            <div class="impact-section" data-section="innovation">
                <div class="section-label">STRATEGIC INNOVATION</div>
                
                <!-- North Star Projects - Large tile -->
                <div class="impact-tile tile-large tile-northstar" data-animate="shine" style="grid-column: span 3;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">Multiple</div>
                                <div class="tile-label">North Star Initiatives</div>
                            </div>
                        </div>
                        <div class="tile-detail">Launched & influenced product direction</div>
                    </div>
                </div>
                
                <!-- Overlay Initiative - Medium tile -->
                <div class="impact-tile tile-medium tile-overlay" data-animate="rotate" style="grid-column: span 3;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">VP-Sponsored</div>
                                <div class="tile-label">Overlay Platform</div>
                            </div>
                        </div>
                        <div class="tile-detail">Self-initiated cross-app intelligence</div>
                    </div>
                </div>
                
                <!-- MAP Framework - Medium tile -->
                <div class="impact-tile tile-medium tile-map" data-animate="flip" style="grid-column: span 2;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">MAP</div>
                                <div class="tile-label">Framework Design</div>
                            </div>
                        </div>
                        <div class="tile-detail">New methodology proposal</div>
                    </div>
                </div>
                
                <!-- Org Design - Large tile -->
                <div class="impact-tile tile-large tile-orgdesign" data-animate="stack" style="grid-column: span 4;">
                    <div class="tile-inner">
                        <div class="tile-mobile-wrapper">
                            <div class="tile-mobile-content">
                                <div class="tile-value">Platform Strategy</div>
                                <div class="tile-label">Organizational Insights</div>
                            </div>
                        </div>
                        <div class="tile-detail">Identified structural barriers to platform adoption</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
