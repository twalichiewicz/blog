<!-- Impact Modal -->
<div id="impact-modal" class="impact-modal" style="display: none;">
    <div class="impact-modal-backdrop" onclick="closeImpactModal()"></div>
    <div class="impact-modal-sheet">
        <div class="impact-modal-header">
            <button class="impact-modal-close minimal-close" onclick="closeImpactModal()" aria-label="Close impact report">
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20" fill="none">
                    <path d="M15 5L5 15M5 5l10 10" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
            </button>
            <div class="impact-modal-title">Design Impact Report 2025</div>
            <button class="impact-modal-party-mode" onclick="togglePartyMode()" aria-label="Toggle party mode" title="Activate party mode">
                <svg class="party-icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M5 3v4M3 5h4M6 17v4m-2-2h4m5-16l2.286 6.857L21 12l-5.714 2.143L13 21l-2.286-6.857L5 12l5.714-2.143L13 3z"/>
                </svg>
                <span class="party-text">Party</span>
                <audio id="impact-audio" loop>
                    <source src="<%- url_for('/media/deepHouse.mp3') %>" type="audio/mpeg">
                </audio>
            </button>
        </div>
        <div class="impact-grid">
            <div class="impact-character" data-impact-character data-archetype="operator" style="--level-progress: 0.72;">
                <div class="impact-character-header">
                    <div class="impact-character-kicker">CHARACTER</div>
                    <div class="impact-character-header-row">
                        <div class="impact-character-identity">
                            <div class="impact-character-name">Thomas Walichiewicz</div>
                            <div class="impact-character-role">Principal Product Designer · AI/ML + Behavioral</div>
                        </div>
                        <div class="impact-character-level">
                            <div class="impact-character-level-badge">LVL <span class="impact-character-level-number">27</span></div>
                            <div class="impact-character-progress" role="progressbar" aria-label="Level progress" aria-valuenow="72" aria-valuemin="0" aria-valuemax="100">
                                <div class="impact-character-progress-fill"></div>
                            </div>
                        </div>
                    </div>
                    <div class="impact-character-selector" role="tablist" aria-label="Archetype">
                        <button type="button" class="impact-character-chip is-active" data-archetype="operator" role="tab" aria-selected="true">Operator</button>
                        <button type="button" class="impact-character-chip" data-archetype="architect" role="tab" aria-selected="false">Architect</button>
                        <button type="button" class="impact-character-chip" data-archetype="behavior" role="tab" aria-selected="false">Behavior</button>
                    </div>
                </div>
                <div class="impact-character-body">
                    <div class="impact-character-viewport" aria-label="3D character preview">
                        <canvas class="impact-character-canvas" aria-hidden="true"></canvas>
                        <img class="impact-character-fallback" src="/img/profilePhoto.jpeg" alt="Thomas Walichiewicz" loading="lazy">
                        <div class="impact-character-hint">Drag</div>
                    </div>
                    <div class="impact-character-stats" aria-label="Character stats">
                        <div class="impact-character-stat">
                            <div class="impact-character-stat-label">Core</div>
                            <div class="impact-character-stat-value">Systems thinking</div>
                        </div>
                        <div class="impact-character-stat">
                            <div class="impact-character-stat-label">Special</div>
                            <div class="impact-character-stat-value">Prototype → Prod</div>
                        </div>
                        <div class="impact-character-badges" aria-label="Highlights">
                            <span class="impact-character-badge">IC by choice</span>
                            <span class="impact-character-badge">Platform scale</span>
                            <span class="impact-character-badge">AI‑native</span>
                        </div>
                    </div>
                </div>
            </div>
